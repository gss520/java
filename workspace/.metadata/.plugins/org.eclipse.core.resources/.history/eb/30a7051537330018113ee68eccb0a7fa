package cn.guo.oop.inherit;

public class Animal2 {
	String eye;
	
	public void run(){
		System.out.println("跑跑！");
	}
	
	public void eat(){
		System.out.println("吃吃！");
	}
	
	public void sleep(){
		System.out.println("zzzzzz");
		
	}
	public Animal2(){
		super();
		System.out.println("创建一个动物！");
	}
}
